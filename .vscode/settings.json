{
  "prettier.disableLanguages": [],
  "vetur.experimental.templateInterpolationService": true,
  "eslint.validate": [
    "javascript",
    {
      "autoFix": true,
      "language": "vue"
    }
  ],
  "editor.formatOnSave": true,
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "window.zoomLevel": 0,
  "endOfLine": "auto",
  "eslint.autoFixOnSave": true,
  "files.associations": {
    "*.vue": "vue"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}